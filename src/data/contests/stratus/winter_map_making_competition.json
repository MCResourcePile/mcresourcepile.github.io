    "panels": [
        {
            "label": "Details and Guidelines",
            "size": "half",
            "rows": [
                {"content": "Information Thread: <a href=\"https://stratus.network/forums/topics/5c4138059e299e00010050f0\" target=\"_blank\">https://stratus.network/forums/topics/5c4138059e299e00010050f0</a>"},
                {"content": "Maps could be any gamemode, but must be between <code>4v4</code> to <code>20v20</code>."},
                {"content": "Maps must be in a team format, no FFA."},
                {"content": "XML documents were to be included with the submission. XML must be at least protocol <code>1.4.0</code>."},
                {"content": "Authors were allowed to be author on up to 3 maps, but only their top scored map was counted."},
                {"content": "Maps previously submitted to Stratus Network could not be entered into this contest."}
            ]
        },
        {
            "label": "Prizes",
            "size": "half",
            "rows": [
                {"label": "First Place",  "content": "Strato, 5x Set Next tokens, winner trophy, winner tag on discord, guaranteed spot in Rotatation Update #20"},
                {"label": "Second Place", "content": "3 weeks of Nimbus, 5x Set Next tokens, runner up tag on discord"},
                {"label": "Third Place",  "content": "3 weeks of Nimbus, 3x Set Next tokens"}
            ]
        }
    ],
    "results_info": "Results Thread: <a href=\"https://stratus.network/forums/topics/5cacc62d5dfb110001002ace\" target=\"_blank\">https://stratus.network/forums/topics/5cacc62d5dfb110001002ace</a>",
